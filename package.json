            },
            "glslConstModifier": {
              "type": "string",
              "default": ""
            },
            "glslAssignmentOperator": {
              "type": "string",
              "default": ""
            },
            "glslArithmeticOperator": {
              "type": "string",
              "default": ""
            },
            "glslSupportFunction": {
              "type": "string",
