{
  "name": "bitfrost-code-theme",
  "displayName": "Bifröst",
  "description": "A VSCode Theme with refined color mappings for the Odin Programming Language and GLSL",
  "version": "1.0.2",
  "publisher": "TheCodeTherapy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheCodeTherapy/bifrost/issues"
  },
  "homepage": "https://github.com/TheCodeTherapy/bifrost",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheCodeTherapy/bifrost"
  },
  "engines": {
    "vscode": "^1.76.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "browser": "./out/web/extension.js",
  "categories": [
    "Themes"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "eslintConfig": {
    "extends": [
      "@mskelton",
      "@mskelton/eslint-config/typescript"
    ],
    "plugins": [
      "html"
    ],
    "rules": {
      "sort/imports": "off"
    }
  },
  "scripts": {
    "docs": "docsify serve docs",
    "build:theme": "ts-node --project scripts/tsconfig.json scripts/generate-theme.ts",
    "build:ts": "tsc -p src",
    "build": "yarn build:sass && yarn build:theme && yarn build:ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "watch:theme": "nodemon ./scripts/generate-theme.ts",
    "watch:ts": "yarn build:ts -w",
    "vscode:prepublish": "yarn build && yarn package-web",
    "package": "vsce package",
    "build:sass": "yarn sass --no-source-map scss:styles && yarn sass --no-source-map scss:styles/origin",
    "compile-web": "webpack --config ./build/web-extension.webpack.config.js",
    "watch-web": "webpack --watch --config ./build/web-extension.webpack.config.js",
    "package-web": "webpack --mode production --devtool hidden-source-map --config ./build/web-extension.webpack.config.js"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "contributes": {
    "markdown.previewStyles": [
      "./styles/base-inside.css",
      "./styles/markdown-inside.css",
      "./styles/atom-one-dark-inside.css"
    ],
    "commands": [
      {
        "command": "bifrost.showChangelog",
        "title": "%description.bifrost.showChangelog%",
        "category": "Bifröst"
      },
      {
        "command": "bifrost.setVivid",
        "title": "%description.bifrost.setVivid%",
        "category": "Bifröst"
      },
      {
        "command": "bifrost.cancelVivid",
        "title": "%description.bifrost.cancelVivid%",
        "category": "Bifröst"
      },
      {
        "command": "bifrost.setItalic",
        "title": "%description.bifrost.setItalic%",
        "category": "Bifröst"
      },
      {
        "command": "bifrost.cancelItalic",
        "title": "%description.bifrost.cancelItalic%",
        "category": "Bifröst"
      },
      {
        "command": "bifrost.setBold",
        "title": "%description.bifrost.setBold%",
        "category": "Bifröst"
      },
      {
        "command": "bifrost.cancelBold",
        "title": "%description.bifrost.cancelBold%",
        "category": "Bifröst"
      }
    ],
    "themes": [
      {
        "label": "Bifröst",
        "uiTheme": "vs-dark",
        "path": "./themes/Bifrost.json"
      },
      {
        "label": "Bifröst Flat",
        "uiTheme": "vs-dark",
        "path": "./themes/Bifrost-flat.json"
      },
      {
        "label": "Bifröst Darker",
        "uiTheme": "vs-dark",
        "path": "./themes/Bifrost-darker.json"
      },
      {
        "label": "Bifröst Mix",
        "uiTheme": "vs-dark",
        "path": "./themes/Bifrost-mix.json"
      },
      {
        "label": "Bifröst Night Flat",
        "uiTheme": "vs-dark",
        "path": "./themes/Bifrost-night-flat.json"
      }
    ],
    "configuration": {
      "title": "Bifröst",
      "properties": {
        "bifrost.editorTheme": {
          "type": "string",
          "default": "Bifröst",
          "markdownDescription": "%description.bifrost.editorTheme%",
          "enum": [
            "Bifröst",
            "Bifröst Flat",
            "Nord",
            "Solarized",
            "Solarized Flat",
            "Shadow",
            "Panda",
            "Ayu",
            "VS Code",
            "Just Black",
            "Bifröst Darker",
            "Bifröst Mix",
            "Ocean",
            "Tokyo",
            "Retro",
            "Gnome",
            "Dark Modern",
            "Bifröst Night Flat"
          ]
        },
        "bifrost.italic": {
          "type": "boolean",
          "default": true,
          "description": "%description.bifrost.italic%"
        },
        "bifrost.vivid": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%description.bifrost.vivid%"
        },
        "bifrost.bold": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%description.bifrost.bold%"
        },
        "bifrost.markdownStyle": {
          "type": "boolean",
          "default": true,
          "description": "%description.bifrost.markdownStyle%"
        },
        "bifrost.color": {
          "type": "object",
          "scope": "resource",
          "default": {
            "arithmeticOperator": "",
            "enumConstants": "",
            "assignmentOperators": "",
            "blockDelimiters": "",
            "constants": "",
            "markupElements": "",
            "embeddedCodeDelimiters": "",
            "illegalSyntax": "",
            "functionNames": "",
            "illegalTokens ": "",
            "comments": "",
            "numericConstants": "",
            "pointerOperators": "",
            "storageTypes": "",
            "strings": "",
            "typeAnnotation": "",
            "variables": "",
            "relationalOperators": "",
            "typeNames": "",
            "namespace": "",
            "enumType": "",
            "structType": "",
            "parameters": "",
            "members": "",
            "keywords": "",
            "modifiers": "",
            "properties": "",
            "methods": "",
            "glslPreprocessorDirective": "",
            "glslNumericConstant": "",
            "glslStorageModifier": "",
            "glslStorageType": "",
            "glslConstModifier": "",
            "glslAssignmentOperator": "",
            "glslArithmeticOperator": "",
            "glslSupportFunction": "",
            "glslSupportVariable": "",
            "glslPrecisionModifier": "",
            "glslBitwiseOperator": "",
            "glslComparativeOperator": "",
            "glslIncrementDecrementOperator": "",
            "glslDefaultText": ""
          },
          "properties": {
            "arithmeticOperator": {
              "type": "string",
              "default": ""
            },
            "assignmentOperators": {
              "type": "string",
              "default": ""
            },
            "blockDelimiters": {
              "type": "string",
              "default": ""
            },
            "constants": {
              "type": "string",
              "default": ""
            },
            "variables": {
              "type": "string",
              "default": ""
            },
            "markupElements": {
              "type": "string",
              "default": ""
            },
            "illegalSyntax": {
              "type": "string",
              "default": ""
            },
            "strings": {
              "type": "string",
              "default": ""
            },
            "illegalTokens ": {
              "type": "string",
              "default": ""
            },
            "comments": {
              "type": "string",
              "default": ""
            },
            "pointerOperators": {
              "type": "string",
              "default": ""
            },
            "functionNames": {
              "type": "string",
              "default": ""
            },
            "storageTypes": {
              "type": "string",
              "default": ""
            },
            "numericConstants": {
              "type": "string",
              "default": ""
            },
            "embeddedCodeDelimiters": {
              "type": "string",
              "default": ""
            },
            "typeAnnotation": {
              "type": "string",
              "default": ""
            },
            "typeNames": {
              "type": "string",
              "default": ""
            },
            "namespace": {
              "type": "string",
              "default": ""
            },
            "enumType": {
              "type": "string",
              "default": ""
            },
            "structType": {
              "type": "string",
              "default": ""
            },
            "parameters": {
              "type": "string",
              "default": ""
            },
            "members": {
              "type": "string",
